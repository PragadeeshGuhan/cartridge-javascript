{
  "name": "cartridge-javascript",
  "version": "0.3.0",
  "description": "JavaScript module for the cartridge project",
  "keywords": [
    "gulp",
    "dependency",
    "cartridge",
    "cartridge-module",
    "cartridge-task",
    "javascript"
  ],
  "homepage": "https://github.com/cartridge/cartridge-javascript",
  "bugs": {
    "url": "https://github.com/cartridge/cartridge-javascript/issues"
  },
  "license": "MIT",
  "author": "Code Computerlove <npm@codecomputerlove.co.uk>",
  "main": "task.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cartridge/cartridge-javascript.git"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "preuninstall": "node scripts/preuninstall.js"
  },
  "dependencies": {
    "cartridge-module-util": "~0.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-if": "^2.0.0",
    "gulp-jsdoc3": "^0.2.1",
    "gulp-jshint": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.1.0",
    "yargs": "^4.3.2"
  },
  "devDependencies": {
    "chai": "1.10.0",
    "chai-fs": "^0.1.0",
    "cz-conventional-changelog": "^1.1.5",
    "fs-extra": "^0.26.7",
    "ghooks": "^1.2.1",
    "mocha": "^2.4.5",
    "validate-commit-msg": "^2.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    }
  }
}
